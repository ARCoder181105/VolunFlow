    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash.merge": "^4.6.2",
